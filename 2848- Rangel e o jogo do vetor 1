
#include <iostream>

using namespace std;

int main()
{
    int n,q,l,r,aux,menor,cont;
    
    cin>>n;
    int v[n];
    cin>>q;
    for(int i=1;i<=n;i++)
    {
        cin>>v[i];
    }
    for(i=0;i<q;i++)//quantidade de rodadas
    {
        cin>>l>>r;//ler qual vai ser o tamanho do vetor
        cin>>menor;//qual vai ser o k-esimo menor
