#include <iomanip>
#include <string>

using namespace std;

class Funcionario{
	private: 
	      string nome;
	      string cpf;
	public:
	      getnome();
	      setnome();
	      getcpf();
	      setcpf()
};
     
class Assistente : private Funcionario{
	private: 
	       int matricula;
	public: 
	       getmatricula();
	       setmatricula();
};
 
 class Tecnico : private Assistente  {	  
	private:
	       double bonus;
	protected:
	       getbonus();
	       setbonus();
};

class Administrativo : private Assistente{
	private:
	       string turno;
	protected: 
	       getturno();
	       setturno();
	       
